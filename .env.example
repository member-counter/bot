# Here is the all env variables that the bot needs to fully function, if you need a minimal configuration, check the file .env.minexample

# development or production
    NODE_ENV=production 
    DEBUG=false
    PREMIUM_BOT=true

# General discord related configuration
    DISCORD_CLIENT_ID=
    DISCORD_CLIENT_SECRET=
    DISCORD_TOKEN=
    DISCORD_PREFIX=mc!
    DISCORD_DEFAULT_LANG=en_US
    DISCORD_BOT_PERMISSIONS=314453
    DISCORD_OFFICIAL_SERVER_ID=614777317733957632
    DISCORD_OFFICIAL_SERVER_URL=https://discord.gg/g4MfV6N
    REWARD_ROLE_ID=614817990407487540

    ## Here must be at least your discord ID
    BOT_OWNERS=343884247263608832

    ## Seconds to wait before update some counters, be careful setting this to lower values when your bot serves a large amount of users
    TIME_BETWEEN_EVERY_UPDATECOUNTER=1

# The mongodb uri to access the db, if it is hosted in the same machine, you typically set this to mongodb://127.0.0.1:27017/memberCounter
    DB_URI=


# Website configuration (front-end not included in this repo)
    # A website homepage
    WEBSITE=https://member-counter.eduardozgz.com/

# Donations
    DONATION_URL=https://member-counter.eduardozgz.com/donate


# Stats
    SEND_BOT_STATS=false

    # https://top.gg configuration
    DBL_TOKEN=
    DBL_WH_SECRET=

    # https://discord.bots.gg/ configuration
    DBGG_TOKEN=

    # https://discord.boats/ configuration
    DBOATS_TOKEN=
    DBOATS_WH_SECRET=

    # https://discordbot.world/ configuration
    DBWORLD_TOKEN=

    # https://bots.ondiscord.xyz/ configuration
    BOND_TOKEN=

    # https://botsfordiscord.com/ configuration
    BFD_TOKEN=
    BFD_WH_SECRET=