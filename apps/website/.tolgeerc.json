{
  "$schema": "https://docs.tolgee.io/cli-schema.json",
  "projectId": 4,
  "apiUrl": "https://tolgee.membercounter.app/",
  "format": "JSON_I18NEXT",
  "push": {
    "files": [
      {
        "path": "./src/i18n/locales/en-US/main.json",
        "language": "en-US",
        "namespace": "main"
      }
    ],
    "forceMode": "OVERRIDE"
  },
  "pull": {
    "path": "./src/i18n/locales",
    "namespaces": ["main"],
    "supportArrays": true,
    "fileStructureTemplate": "{languageTag}/{namespace}.{extension}"
  }
}
