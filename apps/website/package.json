{
  "name": "@mc/website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "make:supported-emojis": "tsx ./src/app/dashboard/servers/[guildId]/TemplateEditor/Emoji/makeSupportedEmojis.ts && prettier --write ./src/app/dashboard/servers/[guildId]/TemplateEditor/Emoji/supportedEmojis.ts",
    "make:twemoji-map": "tsx ./src/app/dashboard/servers/[guildId]/TemplateEditor/Emoji/makeTwemojiMap.ts",
    "prebuild": "pnpm run generate-i18next-res && turbo run make:twemoji-map",
    "build": "pnpm with-env next build --experimental-build-mode compile",
    "clean": "git clean -xdf .next .turbo node_modules",
    "predev": "pnpm run prebuild",
    "dev": "pnpm with-env next dev",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "start": "pnpm with-env next start",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --",
    "generate-i18next-res": "i18next-resources-for-ts interface -i ./src/i18n/locales/en-US -o ./src/@types/resources.d.ts",
    "tolgee:push": "pnpm with-env tolgee push",
    "tolgee:pull": "pnpm with-env tolgee pull"
  },
  "dependencies": {
    "@discordjs/rest": "^2.3.0",
    "@mc/common": "workspace:*",
    "@mc/db": "workspace:*",
    "@mc/logger": "workspace:*",
    "@mc/redis": "workspace:*",
    "@mc/services": "workspace:*",
    "@mc/telemetry": "workspace:*",
    "@mc/trpc-telemetry": "workspace:*",
    "@mc/ui": "workspace:*",
    "@mc/validators": "workspace:*",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "^5.74.3",
    "@trpc/client": "^11.1.0",
    "@trpc/react-query": "^11.1.0",
    "@trpc/server": "^11.1.1",
    "@tsparticles/basic": "^3.5.0",
    "@tsparticles/react": "^3.0.0",
    "@twemoji/api": "^15.1.0",
    "@uidotdev/usehooks": "^2.4.1",
    "accept-language": "^3.0.18",
    "discord-api-types": "^0.37.83",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-resources-to-backend": "^1.2.1",
    "ioredis": "^5.4.1",
    "iron-session": "^8.0.1",
    "is-hotkey": "^0.2.0",
    "iso-lang-codes": "^2.1.1",
    "lodash": "^4.17.21",
    "lucide-react": "^0.475.0",
    "next": "15.2.4",
    "next-nprogress-bar": "^2.3.14",
    "node-loader": "^2.1.0",
    "pretty-bytes": "^6.1.1",
    "prismjs": "^1.30.0",
    "react": "19.0.0",
    "react-cookie": "^7.1.4",
    "react-dom": "19.0.0",
    "react-i18next": "^15.0.0",
    "react-text-transition": "^3.1.0",
    "server-only": "^0.0.1",
    "sharp": "^0.33.5",
    "slate": "^0.103.0",
    "slate-history": "^0.100.0",
    "slate-react": "^0.104.0",
    "superjson": "^2.2.1",
    "uuid": "^9.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@mc/eslint-config": "workspace:*",
    "@mc/prettier-config": "workspace:*",
    "@mc/tailwind-config": "workspace:*",
    "@mc/tsconfig": "workspace:*",
    "@tolgee/cli": "^2.8.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/is-hotkey": "^0.1.10",
    "@types/lodash": "^4.17.1",
    "@types/node": "^22.10.10",
    "@types/prismjs": "^1.26.4",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "@types/uuid": "^9.0.8",
    "dotenv-cli": "^7.4.1",
    "eslint": "^9.19.0",
    "i18next-resources-for-ts": "^1.5.0",
    "jimp": "^1.6.0",
    "jiti": "^2.4.2",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "prettier": "@mc/prettier-config"
}
