{
  "extends": "@mc/tsconfig/internal-package.json",
  "compilerOptions": {
    "tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"],
      "~/env": ["./env"]
    }
  },
  "include": [
    "*.ts",
    "src",
  ],
  "exclude": ["node_modules"]
}
